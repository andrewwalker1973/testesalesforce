            </connector>
            <label>Separator Present At End</label>
        </rules>
    </decisions>
    <decisions>
        <name>AW_CheckForSeparatorAtStart</name>
        <label>Check For Separator At Start</label>
        <locationX>424</locationX>
        <locationY>363</locationY>
        <defaultConnector>
            <targetReference>AW_CheckForSeparatorAtEnd</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>AW_SeparatorPresentAtStart</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AW_SeparatorAtStart</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
