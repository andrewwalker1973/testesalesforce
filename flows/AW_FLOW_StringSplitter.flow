    <decisions>
        <name>AW_CheckForNextSeparatorLocation</name>
        <label>Check For Next Separator Location</label>
        <locationX>426</locationX>
        <locationY>807</locationY>
        <defaultConnector>
            <targetReference>AW_AddLeftStringToCollectionVariable</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>AW_LocationIsZero</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AW_NextSeparatorLocation</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AW_AddRightStringToCollectionVariable</targetReference>
            </connector>
            <label>Location Is Zero</label>
        </rules>
    </decisions>
    <decisions>
        <name>AW_CheckForSeparatorAtEnd</name>
        <label>Check For Separator At End</label>
        <locationX>426</locationX>
        <locationY>585</locationY>
        <defaultConnector>
