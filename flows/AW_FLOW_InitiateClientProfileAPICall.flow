    <description>Process to initiate outbound call to Mulesoft when client profile is updated in Salesforce</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
	[Account].RecordType.DeveloperName = &apos;AW_PersonAccount&apos;,
	NOT($Setup.AW_FeatureSwitches__c.AW_DisableAccountAPICall__c),
	NOT(ISBLANK([Account].Owner.AW_UACFID__c)),
	NOT(ISBLANK([Account].Owner.AW_ConsultantCode__c )),	
	OR
	(
		ISNEW(),
		AND
		(
			NOT(ISNEW()),
			OR
			(
				ISCHANGED([Account].Phone),
				ISCHANGED([Account].PersonMobilePhone),
				ISCHANGED([Account].PersonBirthdate),
				ISCHANGED([Account].PersonEmail),
				ISCHANGED([Account].FirstName),
				ISCHANGED([Account].FinServ__Gender__pc),
				ISCHANGED([Account].PersonHomePhone),
				ISCHANGED([Account].AW_Income__pc),
				ISCHANGED([Account].FinServ__LanguagesSpoken__pc),
				ISCHANGED([Account].FinServ__MotherMaidenName__pc),
				ISCHANGED([Account].FinServ__MaritalStatus__pc),
				ISCHANGED([Account].AW_MedicalAidNumber__pc),
				ISCHANGED([Account].AW_PB_OccupationName__pc),
				ISCHANGED([Account].AW_PassportCountryOfIssue__pc ),
				ISCHANGED([Account].AW_IdentificationNumber__pc),
				ISCHANGED([Account].BillingStreet),
				ISCHANGED([Account].BillingCity),
				ISCHANGED([Account].BillingState),
				ISCHANGED([Account].BillingPostalCode),
				ISCHANGED([Account].BillingCountry),
				ISCHANGED([Account].ShippingStreet),
				ISCHANGED([Account].ShippingCity),
				ISCHANGED([Account].ShippingState),
				ISCHANGED([Account].ShippingPostalCode),
				ISCHANGED([Account].ShippingCountry),
				ISCHANGED([Account].MiddleName),
				ISCHANGED([Account].AW_Smoker__pc),
				ISCHANGED([Account].LastName),
				ISCHANGED([Account].AW_TaxNumber__pc),
				ISCHANGED([Account].Salutation)
			)
		)
	)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
	{!myVariable_current.RecordType.DeveloperName} = &apos;AW_PersonAccount&apos;,
