<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <apiVersion>49.0</apiVersion>
    <assignments>
        <description>Assign the Manager name and Manager Id to variables which are used in Redirect Lead screen</description>
        <name>Assign_Manager_Details</name>
        <label>Assign Manager Details</label>
        <locationX>586</locationX>
        <locationY>204</locationY>
        <assignmentItems>
            <assignToReference>ManagerName</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>ManagerDetails.Name</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ManagerId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>ManagerDetails.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Redirect</targetReference>
        </connector>
    </assignments>
    <assignments>
        <description>Assign the Manager Name and Manager Id to the variables which are further used in Redirect Lead screen</description>
        <name>Assign_Queue_Details</name>
        <label>Assign Queue Details</label>
        <locationX>589</locationX>
        <locationY>376</locationY>
        <assignmentItems>
            <assignToReference>ManagerName</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue></stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ManagerId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>QueueDetails.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Redirect</targetReference>
        </connector>
    </assignments>
    <choices>
        <name>ChoiceSelected</name>
        <choiceText>select</choiceText>
        <dataType>String</dataType>
        <value>
            <elementReference>Reason</elementReference>
        </value>
    </choices>
    <constants>
        <description>Constant for Queue</description>
        <name>Queue</name>
        <dataType>String</dataType>
        <value>
            <stringValue>Queue</stringValue>
        </value>
    </constants>
    <constants>
        <description>This constant hold the developer name for a queue</description>
        <name>UnallocatedQueueDeveloperName</name>
        <dataType>String</dataType>
        <value>
            <stringValue>AW_Unallocated</stringValue>
        </value>
    </constants>
    <constants>
        <description>url to navigate after flow stops</description>
        <name>url</name>
        <dataType>String</dataType>
        <value>
            <stringValue>/00Q/o</stringValue>
        </value>
    </constants>
    <decisions>
        <name>Check_if_Manager_exist</name>
        <label>Check if Manager exist</label>
        <locationX>382</locationX>
        <locationY>206</locationY>
        <defaultConnector>
            <targetReference>Assign_Manager_Details</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Manager_does_not_exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UserDetails.ManagerId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Unallocated_Queue</targetReference>
            </connector>
            <label>Manager does not exist</label>
        </rules>
    </decisions>
    <description>This flow is used from &quot;Redirect Lead&quot; Quick Action from Lead record page. It redirects a lead to the Manager of Adviser, if manager exist otherwise put it in unallocated queue</description>
    <dynamicChoiceSets>
        <description>The resource holds the reasons to redirect a lead.</description>
        <name>RedirectReason</name>
        <dataType>Picklist</dataType>
        <displayField xsi:nil="true"/>
        <object xsi:nil="true"/>
        <picklistField>AW_RedirectReason__c</picklistField>
        <picklistObject>Lead</picklistObject>
    </dynamicChoiceSets>
    <interviewLabel>Redirect Lead {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Redirect Lead</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>Flow</processType>
    <recordLookups>
        <description>Get the current Lead record for further processing</description>
        <name>Get_Lead_Record</name>
        <label>Get Lead Record</label>
        <locationX>146</locationX>
        <locationY>50</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Get_User_Record</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <object>Lead</object>
        <outputReference>LeadDetails</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>OwnerId</queriedFields>
    </recordLookups>
    <recordLookups>
        <description>Get the record for Manager of the owner of Lead</description>
        <name>Get_Manager_Record</name>
        <label>Get Manager Record</label>
        <locationX>390</locationX>
        <locationY>50</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Check_if_Manager_exist</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>UserDetails.ManagerId</elementReference>
            </value>
        </filters>
        <object>User</object>
        <outputReference>ManagerDetails</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>Name</queriedFields>
    </recordLookups>
    <recordLookups>
        <description>Get unallocated queue. This queue hold the record with incomplete data for proper allocation</description>
        <name>Get_Unallocated_Queue</name>
        <label>Get Unallocated Queue</label>
        <locationX>392</locationX>
        <locationY>376</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Assign_Queue_Details</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>DeveloperName</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>UnallocatedQueueDeveloperName</elementReference>
            </value>
        </filters>
        <filters>
            <field>Type</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>Queue</elementReference>
            </value>
        </filters>
        <object>Group</object>
        <outputReference>QueueDetails</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>DeveloperName</queriedFields>
    </recordLookups>
    <recordLookups>
        <description>Get user record for the owner of the Lead</description>
        <name>Get_User_Record</name>
        <label>Get User Record</label>
        <locationX>264</locationX>
        <locationY>50</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Get_Manager_Record</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>LeadDetails.OwnerId</elementReference>
            </value>
        </filters>
        <object>User</object>
        <outputReference>UserDetails</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>ManagerId</queriedFields>
    </recordLookups>
    <recordUpdates>
        <description>Assign the new owner to the lead</description>
        <name>Update_Lead_Record</name>
        <label>Update Lead Record</label>
        <locationX>831</locationX>
        <locationY>305</locationY>
        <connector>
            <targetReference>Finish</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>AW_RedirectReason__c</field>
            <value>
                <elementReference>ChoiceSelected</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>OwnerId</field>
            <value>
                <elementReference>ManagerId</elementReference>
            </value>
        </inputAssignments>
        <object>Lead</object>
    </recordUpdates>
    <screens>
