            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>ClaimantsEmail</name>
            <dataType>String</dataType>
            <defaultValue>
                <elementReference>GetClientAccount.PersonEmail</elementReference>
            </defaultValue>
            <fieldText>Claimant&apos;s Email</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
