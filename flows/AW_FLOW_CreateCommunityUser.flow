<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <decisions>
        <description>Ascertain whether the email address provided by the user has been used before</description>
        <name>AW_DuplicateEmail</name>
        <label>Duplicate Email</label>
        <locationX>668</locationX>
        <locationY>408</locationY>
        <defaultConnector>
            <targetReference>AW_DisplayUserInfo1</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Unique</defaultConnectorLabel>
        <rules>
            <name>AW_Duplicate</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AW_User</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AW_DuplicateEmailMessage1</targetReference>
            </connector>
            <label>Duplicate</label>
        </rules>
    </decisions>
    <decisions>
        <description>This step validates the email existence. If blank or if already exists then the process ends, The user can only be created if the email address is unique</description>
        <name>AW_EmailExists</name>
        <label>Email Exists</label>
        <locationX>663</locationX>
        <locationY>298</locationY>
        <defaultConnector>
            <targetReference>AW_GetUserEmail</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Exists</defaultConnectorLabel>
        <rules>
            <name>AW_Blank</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AW_AccountRecord.PersonEmail</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AW_BlankEmailMessage1</targetReference>
            </connector>
            <label>Blank</label>
        </rules>
    </decisions>
    <decisions>
        <description>Check to see if user has been created before</description>
        <name>AW_UserExists</name>
        <label>User Exists</label>
        <locationX>666</locationX>
        <locationY>127</locationY>
        <defaultConnector>
            <targetReference>AW_EmailExists</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>New User</defaultConnectorLabel>
        <rules>
            <name>AW_ExistingUser</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AW_UserAccountRecord</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AW_DuplicateUserMessage1</targetReference>
            </connector>
            <label>Existing User</label>
        </rules>
    </decisions>
    <decisions>
        <name>Create_User</name>
        <label>Create User?</label>
        <locationX>206</locationX>
        <locationY>116</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Can_Create_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CheckCreateUserPermission</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AW_GetRecordId</targetReference>
            </connector>
            <label>Can Create User</label>
        </rules>
        <rules>
            <name>Cannot_Create_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CheckCreateUserPermission</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Invalid_Permissions</targetReference>
            </connector>
            <label>Cannot Create User</label>
        </rules>
    </decisions>
    <description>Allows the user to create a community user for an existing account</description>
    <formulas>
        <name>CheckCreateUserPermission</name>
        <dataType>Boolean</dataType>
        <expression>{!$Setup.AW_FeatureSwitches__c.AW_Create_Community_User__c} = True</expression>
    </formulas>
    <interviewLabel>Create Community User {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Create Community User</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>Flow</processType>
    <recordLookups>
        <name>AW_GetRecordId</name>
        <label>Get Record Id</label>
        <locationX>408</locationX>
        <locationY>111</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>AW_GetUserAccountID</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <object>Account</object>
        <outputReference>AW_AccountRecord</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>PersonEmail</queriedFields>
        <queriedFields>FirstName</queriedFields>
        <queriedFields>LastName</queriedFields>
        <queriedFields>PersonContactId</queriedFields>
        <queriedFields>AW_CommunityUserCreationInitiated__c</queriedFields>
    </recordLookups>
