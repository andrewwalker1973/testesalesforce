        <developerName>FORMULA4</developerName>
        <isActive>false</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Opp Won Rate</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>if(NUM_WON_OPPORTUNITIES:SUM&gt;0,
ACTUAL_COST:SUM/NUM_WON_OPPORTUNITIES:SUM,null)</calculatedFormula>
