        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Total Cost Per Campaign</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>if(NUM_LEADS:SUM&gt;0,
ACTUAL_COST:SUM/NUM_LEADS:SUM,null)</calculatedFormula>
        <datatype>number</datatype>
        <developerName>FORMULA3</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Campaign Cost Per Lead Generated</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>if(NUM_WON_OPPORTUNITIES:SUM&gt;0,
NUM_OPPORTUNITIES:SUM/NUM_WON_OPPORTUNITIES:SUM,null)</calculatedFormula>
