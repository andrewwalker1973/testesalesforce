    </fields>
    <fields>
        <fullName>AW_SortingDate__c</fullName>
        <description>Used by lightning component to sort life events and goals</description>
        <externalId>false</externalId>
        <formula>DATEVALUE(EventDate)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
