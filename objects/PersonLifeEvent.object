    <validationRules>
        <fullName>AW_EventTypeNotOther</fullName>
        <active>true</active>
        <description>To check if &quot;Other Event Type&quot; is not populated if &quot;Other&quot; event type has not been selected</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL( EventType , &apos;AW_Other&apos;)), 
NOT(ISBLANK( AW_OtherEventType__c ))
)</errorConditionFormula>
        <errorDisplayField>AW_OtherEventType__c</errorDisplayField>
        <errorMessage>Please select &quot;Other&quot; under Event Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AW_Financial_Moment_Type_Selected</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 AW_MomentTypeFinancial__c=true,
 ISBLANK(AW_EstimatedValue__c)
)</errorConditionFormula>
        <errorDisplayField>AW_EstimatedValue__c</errorDisplayField>
        <errorMessage>Financial Moment type is selected, please provide the value in field &apos;Estimated Value&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AW_OtherEventTypeRequired</fullName>
        <active>true</active>
        <description>When Other is selected in Event Type field, the Other Event Type should not be left blank.</description>
        <errorConditionFormula>AND( 
     ISPICKVAL(   EventType , &apos;AW_Other&apos;), 
     ISBLANK(   AW_OtherEventType__c )
   )</errorConditionFormula>
        <errorDisplayField>AW_OtherEventType__c</errorDisplayField>
        <errorMessage>Please enter Other Event Type</errorMessage>
    </validationRules>
