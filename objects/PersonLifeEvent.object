    <fields>
        <fullName>AW_Recurring__c</fullName>
        <defaultValue>false</defaultValue>
        <description>User can select if event should be recurring.</description>
        <externalId>false</externalId>
        <inlineHelpText>Event will be recurred annually.</inlineHelpText>
        <label>Recurring</label>
        <trackHistory>false</trackHistory>
