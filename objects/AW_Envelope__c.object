        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>AW_PackageID__c</fullName>
        <description>This field will be used to store the Package Id of the Envelope being sent.</description>
        <externalId>true</externalId>
        <label>Package ID</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AW_ParentOwnerId__c</fullName>
        <description>Used to store the Parent Owner Id</description>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(AW_Opportunity__r.Id)), AW_Opportunity__r.AW_ParentAccountOwnerId__c,
IF((NOT(ISNULL(AW_Account__r.Id))), AW_Account__r.OwnerId, null))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Parent Owner Id</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AW_StatusLastModifiedDate__c</fullName>
