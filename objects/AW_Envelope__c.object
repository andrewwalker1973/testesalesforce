        <externalId>false</externalId>
        <label>Status Last Modified Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>AW_Status__c</fullName>
        <description>This field represents the lifecycle of an Envelope.</description>
        <externalId>false</externalId>
        <label>Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>New</fullName>
                    <default>true</default>
                    <label>New</label>
                </value>
                <value>
                    <fullName>Queued</fullName>
                    <default>false</default>
                    <label>Queued</label>
                </value>
                <value>
                    <fullName>Sent to Signing Portal</fullName>
                    <default>false</default>
                    <label>Sent to Signing Portal</label>
                </value>
                <value>
                    <fullName>Awaiting Signature</fullName>
                    <default>false</default>
                    <label>Awaiting Signature</label>
                </value>
                <value>
                    <fullName>Declined</fullName>
                    <default>false</default>
                    <label>Declined</label>
                </value>
                <value>
                    <fullName>Completed - Document Signed</fullName>
                    <default>false</default>
                    <label>Completed - Document Signed</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>AW_URL__c</fullName>
        <description>This field will be used to store the package URL returned from Signing Hub.</description>
