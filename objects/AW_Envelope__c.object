        <externalId>false</externalId>
        <label>Date Sent for Signature</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>AW_DateSigned__c</fullName>
        <description>This field will be used to store the date which the Envelope was signed.</description>
        <externalId>false</externalId>
        <label>Date Signed</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>AW_LeadAndReferral__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field will be used to lookup a Lead to which this Envelope would be related to.</description>
        <externalId>false</externalId>
        <label>Lead and Referral</label>
        <referenceTo>Lead</referenceTo>
        <relationshipLabel>Envelopes</relationshipLabel>
        <relationshipName>Envelopes</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>AW_MasterAccountId__c</fullName>
        <description>Used to store the Master Account Id, if Master Account Exists for the Active Account</description>
        <externalId>false</externalId>
        <formula>IF(AND(ISPICKVAL(AW_Account__r.FinServ__Status__c,&apos;Active&apos;), NOT(ISNULL(AW_Account__r.AW_ParentAccount__r.Id ))), AW_Account__r.AW_ParentAccount__r.Id,null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>AW_MasterAccountId</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AW_MasterAccount__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Used to store the Master Account Details</description>
        <externalId>false</externalId>
        <label>MasterAccount</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Envelopes (MasterAccount)</relationshipLabel>
        <relationshipName>Envelopes1</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>AW_Opportunity__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field will be used to lookup an Opportunity to which this Envelope would be related to.</description>
        <externalId>false</externalId>
        <label>Opportunity</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>Envelopes</relationshipLabel>
        <relationshipName>Envelopes</relationshipName>
        <required>false</required>
