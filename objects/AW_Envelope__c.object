    <fields>
        <fullName>AW_Account__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field will be used to lookup an Account to which this Envelope would be related to.</description>
        <externalId>false</externalId>
        <label>Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Envelopes</relationshipLabel>
        <relationshipName>Envelopes</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
