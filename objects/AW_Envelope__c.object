    <validationRules>
        <fullName>AW_CannotEditEnvelopeName</fullName>
        <active>true</active>
        <description>This validation rule will be used to prevent users from changing the Name of the Envelope when the Status is not &quot;New&quot;.</description>
        <errorConditionFormula>AND
(
	 NOT(ISPICKVAL(AW_Status__c, &quot;New&quot;)),
	 ISCHANGED(Name)
)</errorConditionFormula>
        <errorMessage>Cannot change the Envelope Name if Status is not New</errorMessage>
    </validationRules>
