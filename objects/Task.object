    <validationRules>
        <fullName>AW_Orphan_Tasks_Comments_Required</fullName>
        <active>true</active>
        <description>Prevent Orphan Tasks from being closed without comments when certain client interaction outcomes are selected</description>
        <errorConditionFormula>CONTAINS(Subject , &quot;Contact Client&quot;) &amp;&amp; 
ISPICKVAL(Status, &quot;Completed&quot;) &amp;&amp; 
ISPICKVAL( AW_TaskCategory__c, &quot;Orphan&quot;) &amp;&amp; 
ISPICKVAL(AW_ClientInteractionOutcome__c, &quot;Other&quot;) &amp;&amp;
ISBLANK(Description)</errorConditionFormula>
        <errorDisplayField>Description</errorDisplayField>
        <errorMessage>Please fill in the Comments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AW_Orphan_Tasks_Without_Contact_Feedback</fullName>
        <active>true</active>
        <description>Prevent Orphan Tasks from being closed without client interaction outcome</description>
        <errorConditionFormula>CONTAINS(Subject , &quot;Contact Client&quot;) &amp;&amp;  
ISPICKVAL(Status, &quot;Completed&quot;) &amp;&amp; 
ISPICKVAL( AW_TaskCategory__c , &quot;Orphan&quot;) &amp;&amp; ISBLANK(TEXT(AW_ClientInteractionOutcome__c))</errorConditionFormula>
        <errorDisplayField>AW_ClientInteractionOutcome__c</errorDisplayField>
        <errorMessage>Please choose a Client Interaction Outcome</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AW_Orphan_Tasks_Without_Form_Feedback</fullName>
        <active>true</active>
        <description>Prevent Orphan Tasks from being closed without change of consultant form field filled in</description>
        <errorConditionFormula>CONTAINS(Subject , &quot;Change of Consultant Form&quot;) &amp;&amp;  
ISPICKVAL(Status, &quot;Completed&quot;) &amp;&amp; 
ISPICKVAL( AW_TaskCategory__c , &quot;Orphan&quot;) &amp;&amp; ISBLANK(TEXT( AW_Change_of_Consultant_Form__c ))</errorConditionFormula>
        <errorDisplayField>AW_Change_of_Consultant_Form__c</errorDisplayField>
        <errorMessage>Please fill in the Change of Consultant Form field</errorMessage>
    </validationRules>
