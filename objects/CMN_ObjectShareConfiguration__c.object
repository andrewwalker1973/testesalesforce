    <validationRules>
        <fullName>CMN_PreventChangesToFields</fullName>
        <active>true</active>
        <description>Prevent Object API Name, Share From and Share With fields from being modified after the record is created.</description>
        <errorConditionFormula>OR
(
				ISCHANGED(CMN_ObjectAPIName__c),
				ISCHANGED(CMN_ShareFrom__c),
				ISCHANGED(CMN_ShareWith__c)
)</errorConditionFormula>
        <errorMessage>Object API Name, Share From and Share With fields cannot be modified after the record is created. Rather delete old records and create new records.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CMN_PreventSameShareFromShareWith</fullName>
        <active>true</active>
        <description>Prevent same User selected for Share From and Share With fields.</description>
        <errorConditionFormula>CMN_ShareFrom__c = CMN_ShareWith__c</errorConditionFormula>
        <errorMessage>Same User cannot be selected for both Share From and Share With fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CMN_ShareWithMustBeActive</fullName>
        <active>true</active>
        <description>The the user defined in the ShareWith field must be active. Records may only be shared with Active users</description>
        <errorConditionFormula>CMN_ShareWith__r.IsActive = FALSE</errorConditionFormula>
        <errorDisplayField>CMN_ShareWith__c</errorDisplayField>
        <errorMessage>Records can only be shared with Active Users</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>