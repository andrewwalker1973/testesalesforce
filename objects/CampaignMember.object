    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AW_AdviserBranch__c</fullName>
        <description>Non-displayed field used in Campaign Dashboard</description>
        <externalId>false</externalId>
        <formula>IF(LeadId = NULL, Contact.AW_AdviserBranch__c , Lead.AW_Adviser_Branch__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Adviser Branch</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AW_HasClicked__c</fullName>
        <description>If a campaign member has clicked on the campaign</description>
        <externalId>false</externalId>
        <formula>OR(
ISPICKVAL(Status, &apos;Clicked&apos;),
ISPICKVAL(Status, &apos;Responded&apos;)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>HasClicked</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>AW_HasOpened__c</fullName>
        <description>If a campaign member has opened the email</description>
        <externalId>false</externalId>
        <formula>OR(
ISPICKVAL(Status, &apos;Opened&apos;),
ISPICKVAL(Status, &apos;Responded&apos;),
ISPICKVAL(Status, &apos;Clicked&apos;)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>HasOpened</label>
        <type>Checkbox</type>
    </fields>
