    <validationRules>
        <fullName>AW_CampaignMember_Prevent</fullName>
        <active>true</active>
        <description>Prevents users from adding campaign members to a campaign if the campaign is not active or   after the campaign has ended.</description>
        <errorConditionFormula>TODAY() &gt; Campaign.EndDate 
|| Campaign.IsActive &lt;&gt; true
|| ISPICKVAL(Campaign.Status, &apos;Cancelled&apos;)</errorConditionFormula>
        <errorMessage>You may not add campaign members to a campaign which is cancelled, not active or after it has ended.</errorMessage>
    </validationRules>
