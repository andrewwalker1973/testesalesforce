    <validationRules>
        <fullName>AW_Goaltypenorother</fullName>
        <active>true</active>
        <description>When Other is not selected in Type picklist, but Other Goal Type field is filled out.</description>
        <errorConditionFormula>AND
(
	OR
	(
		ISCHANGED(AW_GoalTypeIfOther__c),
		ISNEW()
	),
	
	NOT(ISBLANK(AW_GoalTypeIfOther__c)),
	TEXT(FinServ__Type__c) &lt;&gt; &quot;Other&quot;
)</errorConditionFormula>
        <errorDisplayField>AW_GoalTypeIfOther__c</errorDisplayField>
        <errorMessage>Cannot enter Other Goal Type if Other is not selected as Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AW_OtherGoalType</fullName>
        <active>true</active>
        <description>Prompt to the user to specify the goal when selecting other goal type</description>
        <errorConditionFormula>AND
(
  ISPICKVAL( FinServ__Type__c , &apos;Other&apos;),
  ISBLANK( AW_GoalTypeIfOther__c )  
)</errorConditionFormula>
        <errorDisplayField>AW_GoalTypeIfOther__c</errorDisplayField>
        <errorMessage>Please fill Other Goal Type.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
