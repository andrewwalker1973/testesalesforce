    <validationRules>
        <fullName>SC_Reason_Mandatory_IfRejected</fullName>
        <active>true</active>
        <description>Reason must be provided if an item is set to Rejected, or the item has been Rejected and Reason has been set to blank.</description>
        <errorConditionFormula>AND
(
    RecordType.DeveloperName = &apos;SC_CaseDocument&apos;,
    TEXT(Status) = &apos;Rejected&apos;,
    ISBLANK(CMN_Reason__c),
    OR
    (
        ISCHANGED(CMN_Reason__c),
        ISCHANGED(Status)
    )
)</errorConditionFormula>
        <errorDisplayField>CMN_Reason__c</errorDisplayField>
        <errorMessage>Please select a reason for rejecting the document.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SC_Reason_ReadOnly_IfNotRejected</fullName>
        <active>true</active>
        <description>The Reason field may only be used for items that have been rejected and should be blank if status is updated from Rejected to any other value.</description>
        <errorConditionFormula>AND
(
    RecordType.DeveloperName = &apos;SC_CaseDocument&apos;,
    NOT(ISNEW()),
    ISCHANGED(CMN_Reason__c),
    NOT(TEXT(Status) = &apos;Rejected&apos;),
    NOT(TEXT(PRIORVALUE(Status)) = &apos;Rejected&apos;)
)</errorConditionFormula>
        <errorDisplayField>CMN_Reason__c</errorDisplayField>
        <errorMessage>Reason may only be edited for Rejected Items</errorMessage>
    </validationRules>
