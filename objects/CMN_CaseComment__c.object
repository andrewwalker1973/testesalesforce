    <deploymentStatus>Deployed</deploymentStatus>
    <description>Custom Object used to store all the comments on the case which will be used for integration purpose</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>CMN_Case__c</fullName>
        <description>Custom field used to relate the comments with Case</description>
        <externalId>false</externalId>
        <label>Case</label>
        <referenceTo>Case</referenceTo>
        <relationshipLabel>Case Comments</relationshipLabel>
        <relationshipName>Case_Comments</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>CMN_CommentCreateDate__c</fullName>
        <description>Field used to store either the date sent by the target source system or the date when the comment was created in salesforce</description>
        <externalId>false</externalId>
        <formula>IF( CMN_IsExternalComment__c,  CMN_ExternalCommentCreatedDate__c,  CreatedDate)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Comment Create Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>CMN_CommentCreatedByInformation__c</fullName>
        <description>Used to store either the UACFId or the userID of the found user</description>
        <externalId>false</externalId>
        <label>Comment Created By Information</label>
        <length>32</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CMN_CommentCreatedBy__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Used to store the user information based on the UACF Id received from the Integration</description>
        <externalId>false</externalId>
        <label>Comment Created By</label>
        <referenceTo>User</referenceTo>
        <relationshipName>Case_Comments</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>CMN_Comment__c</fullName>
        <description>Custom field used to store the comments used for integration</description>
        <externalId>false</externalId>
        <label>Comment</label>
        <length>4000</length>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Html</type>
        <visibleLines>25</visibleLines>
    </fields>
    <fields>
        <fullName>CMN_CreatedUser__c</fullName>
        <description>Used to store the user information who created the case comment in url format, so agent can view the created user information</description>
        <externalId>false</externalId>
        <formula>IF(NOT(CMN_IsExternalComment__c) , HYPERLINK(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_260))&amp;CreatedBy.Id, (CreatedBy.FirstName +&quot; &quot;+ CreatedBy.LastName)),IF(LEN(CMN_CommentCreatedByInformation__c) = 18 , HYPERLINK(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_260))&amp;CMN_CommentCreatedBy__r.Id, (CMN_CommentCreatedBy__r.FirstName +&quot; &quot;+ CMN_CommentCreatedBy__r.LastName)), CMN_CommentCreatedByInformation__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Created User</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CMN_ExternalCommentCreatedDate__c</fullName>
        <description>Field used to store the created Date and time for Comments</description>
        <externalId>false</externalId>
        <label>External Comment Created Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>CMN_ExternalCommentId__c</fullName>
        <description>Custom field used to store the external comment Id received from the external systems via integration</description>
        <externalId>false</externalId>
        <label>External Comment Id</label>
        <length>32</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CMN_IsExternalComment__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Field used to identify if the Comment was created from the external system or from Salesforce</description>
        <externalId>false</externalId>
        <label>Is External Comment</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CMN_IsPrivate__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Custom field used to identify if the comments are visible for the user</description>
        <externalId>false</externalId>
        <label>Is Private</label>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CMN_SourceOfOrigin__c</fullName>
        <externalId>false</externalId>
        <formula>IF(CMN_IsExternalComment__c, &apos;Horizon&apos;, &apos;Salesforce&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Source Of Origin</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Case Comment</label>
    <listViews>
