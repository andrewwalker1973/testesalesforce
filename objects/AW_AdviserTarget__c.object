    <fields>
        <fullName>AW_ActualLeadConversionRate__c</fullName>
        <description>Actual Lead Conversion Rate</description>
        <externalId>false</externalId>
        <formula>IF(

	OR(
		AND
		(
			ISBLANK(AW_ActualNumberOfLeadConverted__c),
			ISBLANK(AW_ActualNumberOfLeadOpen__c)
		),
		AW_ActualNumberOfLeadConverted__c+AW_ActualNumberOfLeadOpen__c=0
	),
	NULL,
	AW_ActualNumberOfLeadConverted__c /( AW_ActualNumberOfLeadConverted__c + AW_ActualNumberOfLeadOpen__c )
)</formula>
        <label>Actual Lead Conversion Rate</label>
        <precision>18</precision>
        <required>false</required>
        <scale>3</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
