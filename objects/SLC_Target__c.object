        <fullName>SLC_StretchedTargetP__c</fullName>
        <externalId>false</externalId>
        <formula>IF( CONTAINS(RecordType.Name, &apos;BDM&apos;), SLR_BdmHoldingsRollup__c / SLC_StretchedTarget__c,
IF( CONTAINS(RecordType.Name, &apos;CFM&apos;),  SLI_CFMHoldingsRollup__c / SLC_StretchedTarget__c, 0
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Stretched Target %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
