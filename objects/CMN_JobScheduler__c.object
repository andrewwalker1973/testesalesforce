    <deploymentStatus>Deployed</deploymentStatus>
    <description>An object that can be used to manage CronTriggers (Scheduled Jobs) in a managed manager. Only accessible by Administrators.</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>false</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>CMN_Active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Set this flag too true to ensure the scheduled job is initiated. If an error occurs this flag will be reset too false. In addition when a scheduled job is terminated this flag will also be set to false.</description>
        <externalId>false</externalId>
        <label>Active</label>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CMN_CronExpression__c</fullName>
        <description>This is a Salesforce specific Cron (https://en.wikipedia.org/wiki/Cron) expression that will be used to schedule jobs. The supported syntax can be found here (https://developer.salesforce.com/docs/atlas.en-us.apexcode.meta/apexcode/apex_scheduler.htm)</description>
        <externalId>false</externalId>
        <label>Cron Expression</label>
        <length>40</length>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CMN_Description__c</fullName>
        <description>Enter Brief Description</description>
        <externalId>false</externalId>
        <inlineHelpText>Describes what the Job does and how often it runs</inlineHelpText>
        <label>Description</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>CMN_JobName__c</fullName>
        <description>The name of the scheduled job, composed from Cron expression and Class being run</description>
        <externalId>false</externalId>
        <formula>CMN_SchedulerName__c + &apos; - &apos; + &apos;(&apos; +  CMN_CronExpression__c + &apos;)&apos;</formula>
        <label>Job Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CMN_ScheduleClassName__c</fullName>
        <description>The APEX class that supports the Schedular framework and can be scheduled.</description>
        <externalId>false</externalId>
        <label>Schedule Class Name</label>
        <length>40</length>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CMN_ScheduledJobId__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>The Salesforce Id for the Job that was initiated for the current Job Schedular row.</description>
        <externalId>false</externalId>
        <label>Scheduled Job Id</label>
        <length>18</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>CMN_SchedulerName__c</fullName>
        <description>This is the user friendly name to be be provided when setting up a Schedule</description>
        <externalId>false</externalId>
        <label>Scheduler Name</label>
        <length>40</length>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
